        "examples": [
          true
        ]
      },
      "author": {
        "type": "string",
        "title": "Theme author",
        "description": "The author of the theme",
        "default": "",
        "examples": [
          "John Doe"
        ]
      },
      "editorScheme": {
        "type": "string",
        "title": "Path to the editor schema xml file starting with '/'",
